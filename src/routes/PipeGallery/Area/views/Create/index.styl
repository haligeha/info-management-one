.pipe-area-management
  .management-people
    display: flex
    .people-box
      border: 1px solid #d9d9d9
      height: 230px
      cursor: pointer
      width: 40%
      .box-title
        border: 1px solid #d9d9d9
        padding-left: 35px
        overflow: hidden
        height: 40px
        width: 100%
      .box-content
        border: 1px solid #d9d9d9
        height: 190px
        overflow: auto
        .box-content-border
          border-bottom: 1px solid #fff
          padding: 8px 0
        .box-content-padding
          padding-left: 20px
    .people-arrow   
      height: 230px
      width: 20%
      .arrow-icon
        display: block
        font-size: 20px
        padding: 5px
        background-color: #1890ff
        color: #fff
        margin: 0 25px
        &:first-child
          margin-top: 70px
          margin-bottom: 20px
  .path-way-border
      margin-top: 10px
      border: 1px dashed #1890ff 
      padding: 20px
      border-radius: 5px  
    .operator-container
      margin-left: 53%
    .path-way-line-up
      height :285px
      border-bottom: 1px dashed #1890ff
    .path-way-line-down
      height :86px
      border-bottom: 1px dashed #1890ff
  .path-way-border-input
      border: 1px dashed #1890ff 
      padding: 10px
      border-radius: 5px 